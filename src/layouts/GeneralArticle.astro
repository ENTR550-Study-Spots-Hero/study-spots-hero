---
// Page containing links to each of the building/[building name] pages

import { type CollectionEntry, getCollection } from "astro:content";
import BaseLayout from "../components/BaseLayout.astro";
import StudySpot from "./StudySpot.astro";
// filterFunc is a function that takes a StudySpot and returns a boolean
const { spots, filterFunc } = Astro.props;
---

<BaseLayout>
    <main>
        {spots.filter(filterFunc).map((spot) => <StudySpot {...spot} />)}
    </main>
</BaseLayout>
